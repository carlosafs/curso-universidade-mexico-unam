minha_funcao <- function(x, y, operacao = "soma"){
if (operacao == "soma"){
return(x + y)
}
if (operacao == "subtracao"){
return(x - y)
}
if (operacao == "multiplicacao"){
return(x * y)
}
if (operacao == "divisao"){
return(x / y)
}
}
